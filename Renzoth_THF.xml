	<variables>
		<var name="Set">Idle</var>
		<var name="TH">0</var>
	</variables>
	<inputcommands>
		<cmd input="/idle">
			<setvar name="Set" value="Idle"/>
		</cmd>
		<cmd input="/bc">
			<setvar name="Set" value="BC"/>
		</cmd>
		<cmd input="/eva">
			<setvar name="Set" value="EVA"/>
		</cmd>
		<cmd input="/dex">
			<setvar name="Set" value="SA"/>
		</cmd>
		<cmd input="/agi">
			<setvar name="Set" value="TA"/>
		</cmd>
		<cmd input="/empty">
			<setvar name="Set" value="Empty"/>
		</cmd>
		<cmd input="/thon">
			<setvar name="TH" value="1"/>
		</cmd>
		<cmd input="/thoff">
			<setvar name="TH" value="0"/>
		</cmd>
	</inputcommands>
	<idlegear>
		<equip set="$Set"/>

		<if p_status="Engaged">
			<equip>
				<hands>Dusk Gloves</hands>
				<feet>Dusk Ledelsens</feet>
			</equip>
		</if>

		<if advanced="%p_subjob=NIN">
			<if advanced="$TH=1">
				<equip>
					<main>Harpe</main> 
					<sub>Thief's Knife</sub>
				</equip>
			</if><else>
				<equip>
					<main>Harpe</main>
					<sub>Lyft Jambiya</sub>
				</equip>
			</else>
			
		</if><else>
			<if advanced="$TH=1">
				<equip>
					<main>Thief's Knife</main>
					<sub>Tatami Shield</sub>
				</equip>
			</if><else>
				<equip>
					<main>Harpe</main>
					<sub>Tatami Shield</sub>
				</equip>
			</else>
		</else>

		<if buffactive="Sneak Attack">
			<equip set="SA"/>
		</if><elseif buffactive="Trick Attack">
			<equip set="TA"/>
		</elseif>

		<if eq_main="Warp Cudgel">
			<equip lock="true">
				<main>Warp Cudgel</main>
			</equip>
		</if>

		<if advanced="(%p_joblevel &lt;= 41)">
			<equip set="35sync"/>
		</if>
	</idlegear>

	<preranged>
	</preranged>

	<midranged>
