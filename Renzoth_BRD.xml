				<command>/so "Valor Minuet IV" &lt;stpc&gt;</command>
			</if>
			<elseif advanced="(%p_joblevel >= 43)">
				<command>/so "Valor Minuet III" &lt;stpc&gt;</command>
			</elseif>
			<elseif advanced="(%p_joblevel >= 23)">
				<command>/so "Valor Minuet II" &lt;stpc&gt;</command>
			</elseif>
			<else>
				<command>/so "Valor Minuet" &lt;stpc&gt;</command>
